<Window x:Class="Demo_Fitters_2d.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Demo_Fitters_2d"
        xmlns:CoastControls ="clr-namespace:Coast.Controls;assembly=Coast.Controls"
        mc:Ignorable="d"
        Title="MathGraph" Height="800" Width="1200">
    <Grid>
        <TabControl SelectedIndex="2" TabStripPlacement="Top">
            <TabItem Header="LineFitter">
                <local:LineFitterControl></local:LineFitterControl>
            </TabItem>
            <TabItem Header="CircleFitter">
                <local:CircleFitterControl></local:CircleFitterControl>
            </TabItem>
            <TabItem Header="EllipseFitter">
                <local:EllipseFitterControl></local:EllipseFitterControl>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
